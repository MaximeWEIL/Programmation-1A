
public class PointCol extends Point {
	public int couleur;
	public PointCol(int x, int y, int c){
		super(x,y);   //appel du constructeur de la super-classe
		couleur = c;
	}
	
	public int afficheCouleur(){
		return this.couleur;
	}
	
	public int affichex(){
		return super.x;
	}
	
	public int affichey(){
		return super.y;
	}
}
