#include <stdio.h>
#include <stdlib.h>
#include "arbre.h"

arbre nouvArbre()  // rend lâ€™arbre vide, ici (arbre)NULL
{
	return NULL;
}

arbre enrac(T racine, arbre fg, arbre fd)
{
	arbre* a = (arbre*)malloc(sizeof(struct NOEUD));
	a -> fg = fg;
	a -> fd = fd;
	a -> val = racine;
	return a;
}
